{
  "description": "Custom configuration for TestRail to Jira with specific field requirements",
  "version": "1.0",
  
  "column_mappings": {
    "Summary": "Title",
    "Issue Type": "Test"
  },
  
  "static_values": {
    "Issue Type": "Test"
  },
  
  "transformations": {
    "Summary": "format_summary_with_id"
  },
  
  "required_fields": [
    "Issue Type",
    "Summary"
  ],
  
  "jira_fields": [
    "Issue Type",
    "Summary"
  ],
  
  "validation_rules": {
    "max_summary_length": 255,
    "required_issue_types": ["Test"]
  }
}